<Addin namespace="FSpot"
	version="0.9"
	compatVersion="0.9"
	name="PicasaWeb Export"
	description="Export your photos to PicasaWeb."
	author="F-Spot team"
	url="http://f-spot.org"
	defaultEnabled="true"
	category="Export">

	<Dependencies>
		<Addin id="Core" version="0.9"/>
	</Dependencies>

	<Extension path = "/FSpot/Menus/Exports">
		<ExportMenuItem id="PicasaWeb" _label = "_PicasaWeb..." class = "FSpot.Exporters.PicasaWeb.GoogleExport" />
	</Extension>
</Addin>
