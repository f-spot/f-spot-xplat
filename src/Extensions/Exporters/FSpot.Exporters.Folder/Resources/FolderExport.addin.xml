<Addin namespace="FSpot"
	version="0.9"
	compatVersion="0.9"
	name="Folder Export"
	description="Export your photos to HTML galleries, Original galleries or just to a folder."
	author="F-Spot team"
	url="http://f-spot.org"
	defaultEnabled="true"
	category="Export">

	<Dependencies>
		<Addin id="Core" version="0.9"/>
	</Dependencies>

	<Extension path = "/FSpot/Menus/Exports">
		<ExportMenuItem id="Folder" _label = "F_older..." class = "FSpot.Exporters.Folder.FolderExport" />
	</Extension>
</Addin>
