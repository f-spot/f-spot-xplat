<Addin namespace="FSpot"
	version="0.9"
	compatVersion="0.9"
	name="CD Export"
	description="Burn your photos to CD."
	author="F-Spot team"
	url="http://f-spot.org"
	defaultEnabled="true"
	category="Export">

	<Dependencies>
		<Addin id="Core" version="0.9"/>
	</Dependencies>

	<Extension path = "/FSpot/Menus/Exports">
		<ExportMenuItem id="CD" _label = "_CD..." class = "FSpot.Exporters.CD.CDExport" />
	</Extension>
</Addin>
